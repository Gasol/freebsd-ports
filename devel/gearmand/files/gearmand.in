#!/bin/sh
#
# $FreeBSD: ports/devel/gearmand/files/gearmand.in,v 1.1 2012/08/05 23:19:36 dougb Exp $
#

# PROVIDE: gearmand
# REQUIRE: DAEMON

. /etc/rc.subr

name="gearmand"
rcvar=gearmand_enable
command="%%PREFIX%%/sbin/gearmand"

load_rc_config "$name"
: ${gearmand_enable="NO"}
: ${gearmand_flags="-d -v"}
: ${gearmand_group="daemon"}
: ${gearmand_user="daemon"}

run_rc_command "$1"

